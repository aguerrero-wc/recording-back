generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

// No es necesario definir modelos para probar la conexión.
// Puedes agregar modelos aquí cuando empieces a modelar tus tablas.


